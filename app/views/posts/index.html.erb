<%= render partial: 'layouts/user_header', locals: { user: @user } %>
<%= render partial: 'layouts/section_title', locals: { section_title: 'All Posts' } %>
<div>
  <% @posts.each do |post| %>
    <%= render partial: 'layouts/post_header', locals: { post: post , from: 'post/index' } %>
    <%= render partial: 'layouts/comments_list', locals: { post: post } %>
  <% end %>
</div>
